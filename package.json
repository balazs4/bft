{
  "name": "t",
  "version": "0.0.0-semantic-release",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "t": "t.js"
  },
  "scripts": {
    "dev": "node -r ./index.js example.test.js",
    "expect": "NO_COLOR=true npm run -s dev > example.test.js.expected || true",
    "pretest": "NO_COLOR=true npm run -s dev > example.test.js.actual || true",
    "test": "diff -u example.test.js.expected example.test.js.actual",
    "watch": "npx onchange -i -k '*.js' -- npm run -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balazs4/t.git"
  },
  "keywords": ["test", "test-runner", "minimal", "bloat-free"],
  "author": "balazs4",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/balazs4/t/issues"
  },
  "homepage": "https://github.com/balazs4/t#readme",
  "prettier": {
    "singleQuote": true
  }
}
